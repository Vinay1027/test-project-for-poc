<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceType="cq/gui/components/authoring/dialog"
    mode="edit"
    trackingFeature="core-components:page:v2">
    <content
        granite:class="cq-dialog-content-page"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                granite:class="cq-siteadmin-admin-properties-tabs"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <seo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="SEO"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <canonicalUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter canonical url"
                                        fieldLabel="Canonical URL"
                                        name="./canonicalUrl"/>
                                    <index
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Index, No Index"
                                        name="./index">
                                        <items jcr:primaryType="nt:unstructured">
                                            <index
                                                jcr:primaryType="nt:unstructured"
                                                text="INDEX"
                                                value="index"/>
                                            <noIndex
                                                jcr:primaryType="nt:unstructured"
                                                text="NO INDEX"
                                                value="no index"/>
                                        </items>
                                    </index>
                                    <follow
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Follow, No Follow"
                                        name="./follow">
                                        <items jcr:primaryType="nt:unstructured">
                                            <follow
                                                jcr:primaryType="nt:unstructured"
                                                text="FOLLOW"
                                                value="follow"/>
                                            <noFollow
                                                jcr:primaryType="nt:unstructured"
                                                text="NO FOLLOW"
                                                value="no follow"/>
                                        </items>
                                    </follow>
                                    <seoPriorityOptions
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="SEO Priority Options"
                                        name="./seoPriorityOptions">
                                        <items jcr:primaryType="nt:unstructured">
                                            <_x0030_.1
                                                jcr:primaryType="nt:unstructured"
                                                text="0.1"
                                                value="0.1"/>
                                            <_x0030_.2
                                                jcr:primaryType="nt:unstructured"
                                                text="0.2"
                                                value="0.2"/>
                                            <_x0030_.3
                                                jcr:primaryType="nt:unstructured"
                                                text="0.3"
                                                value="0.3"/>
                                            <_x0030_.4
                                                jcr:primaryType="nt:unstructured"
                                                text="0.4"
                                                value="0.4"/>
                                            <_x0030_.5
                                                jcr:primaryType="nt:unstructured"
                                                text="0.5"
                                                value="0.5"/>
                                            <_x0030_.6
                                                jcr:primaryType="nt:unstructured"
                                                text="0.6"
                                                value="0.6"/>
                                            <_x0030_.7
                                                jcr:primaryType="nt:unstructured"
                                                text="0.7"
                                                value="0.7"/>
                                            <_x0030_.8
                                                jcr:primaryType="nt:unstructured"
                                                text="0.8"
                                                value="0.8"/>
                                            <_x0030_.9
                                                jcr:primaryType="nt:unstructured"
                                                text="0.9"
                                                value="0.9"/>
                                            <_x0031_.0
                                                jcr:primaryType="nt:unstructured"
                                                text="1.0"
                                                value="1.0"/>
                                        </items>
                                    </seoPriorityOptions>
                                    <seoFrequencyOptions
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="SEO Frequency Options"
                                        name="./seoFrequencyOptions">
                                        <items jcr:primaryType="nt:unstructured">
                                            <daily
                                                jcr:primaryType="nt:unstructured"
                                                text="Daily"
                                                value="daily"/>
                                            <weekly
                                                jcr:primaryType="nt:unstructured"
                                                text="Weekly"
                                                value="weekly"/>
                                            <monthly
                                                jcr:primaryType="nt:unstructured"
                                                text="Monthly"
                                                value="monthly"/>
                                        </items>
                                    </seoFrequencyOptions>
                                    <hideInSitemap
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                        fieldDescription="if it is checked then the page will be hidden in sitemap"
                                        name="./hideInSitemap"
                                        text="Hide In Sitemap"
                                        uncheckedValue="{Boolean}false"
                                        value="true"/>
                                    <twitterTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter twitter title"
                                        fieldLabel="Twiter Title"
                                        name="./twitterTitle"/>
                                    <ogImage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldDescription="Og Image"
                                        fieldLabel="Drag And Drop Og Image"
                                        fileNameParameter="./fileName"
                                        fileReferenceParameter="./ogImage"
                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml,image/webp]"
                                        multiple="{Boolean}false"
                                        name="./image"
                                        title="Og Image"
                                        uploadUrl="${suffix.path}"
                                        useFixedInlineToolbar="{Boolean}true"
                                        useHTML5="{Boolean}true"/>
                                    <twitterImage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldDescription="Twitter Image"
                                        fieldLabel="Drag And Drop Twitter Image"
                                        fileNameParameter="./fileName"
                                        fileReferenceParameter="./twitterImage"
                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml,image/webp]"
                                        multiple="{Boolean}false"
                                        name="./image"
                                        title="Twitter Image"
                                        uploadUrl="${suffix.path}"
                                        useFixedInlineToolbar="{Boolean}true"
                                        useHTML5="{Boolean}true"/>
                                    <twitterDescription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter twitter description"
                                        fieldLabel="Twiter Description"
                                        name="./twitterDescription"/>
                                </items>
                            </column>
                        </items>
                    </seo>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
